    <color=#f8f8f2>time</color> <color=#f92672>=</color> <color=#f8f8f2>LogUtils</color><color=#f8f8f2>.</color><color=#a6e22e>readTimestamp</color><color=#f8f8f2>(</color><color=#f8f8f2>logBuffer</color><color=#f8f8f2>,</color> <color=#66d9ef>false</color> <color=#75715e>/* isUnpacked. */</color><color=#f8f8f2>);</color>
    <color=#f8f8f2>repMasterNodeId</color> <color=#f92672>=</color> <color=#f8f8f2>LogUtils</color><color=#f8f8f2>.</color><color=#a6e22e>readInt</color><color=#f8f8f2>(</color><color=#f8f8f2>logBuffer</color><color=#f8f8f2>,</color> <color=#66d9ef>false</color> <color=#75715e>/* unpacked */</color><color=#f8f8f2>);</color>
  <color=#f8f8f2>}</color>

